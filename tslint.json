{
  "extends": ["tslint-config-standard"],
  "rules": {
    "indent": [true, "spaces", 2],
    "no-consecutive-blank-lines": false,
    "no-multi-spaces": false,
    "semicolon": false,
    "space-before-function-paren": false,
    "typedef-whitespace": false,
    "unified-signatures": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-type",
      "check-typecast",
      "check-preblock"
    ]
  }
}
